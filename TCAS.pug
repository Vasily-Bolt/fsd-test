mixin TCAS-colors-block(modificator, values)
  .TCAS-block
    .TCAS-block__left
      .TCAS-block__left--square
    .TCAS-block__right
        h2= values[0]
        p= values[1]

mixin TCAS-headers-block(modificator, values)
  .TCAS-block
    .TCAS-block__left
      .TCAS-block__left--text
        p= values[0]
    .TCAS-block__right
        h2= values[1]
